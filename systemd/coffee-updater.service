[Unit]
Description=Coffee Manager Auto-Updater
After=network-online.target
Wants=network-online.target

[Service]
Type=oneshot
User=coffeelover
Group=coffeelover
WorkingDirectory=/home/coffeelover/CoffeeManager
ExecStart=/usr/bin/python3 /home/coffeelover/CoffeeManager/updater.py
StandardOutput=journal
StandardError=journal
TimeoutStartSec=300

[Install]
WantedBy=multi-user.target


